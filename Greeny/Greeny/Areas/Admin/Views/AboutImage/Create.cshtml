@model AboutImageCreateVM
@{
    ViewData["Title"] = "Create";
}


<div class="container my-5">
    <form method="post" asp-action="Create" enctype="multipart/form-data">

        <input asp-for="Image" class="form-control" />
        <span asp-validation-for="Image" class="text-danger d-block"></span>

        <button type="submit" class="btn btn-inverse-primary btn-fw mt-3">Submit</button>

        <a asp-action="Index" class="btn btn-outline-info btn-fw mt-3">Back</a>
    </form>
</div>


